#!/bin/bash
# ip j채rgi arvuti kontrollimine
echo -n "sisesta vahemiku algus: "
read algus
echo -n "sisesta vahemiku l천pp: "
read lopp
echo -n > ping_tulemus
for((nr=$algus; nr<=$lopp; nr++))
do
	# paneme ip kokku
ip=172.23.13.$nr
# teata,e antud ip pingu
ping -c 1 $ip > /dev/null # v채ljund kustutatud
# kontroll
if [ $? -eq 0 ]; then
# kui $? on 0 - k천ik korras
echo "$ip - ok" >> ping_tulemus
# muidu
else
echo "$ip -not" >> ping_tulemus
fi
done

